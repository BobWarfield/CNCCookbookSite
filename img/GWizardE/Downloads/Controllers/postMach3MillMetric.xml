<?xml version="1.0" encoding="utf-8"?>
<post>
  <incrementalIJ>true</incrementalIJ>
  <modalCenter>false</modalCenter>
  <g51Style>2</g51Style>
  <mach3CannedZ>true</mach3CannedZ>
  <cannedRepeatStyle>0</cannedRepeatStyle>
  <volatilityChoice>Fanuc Style (#500-999)</volatilityChoice>
  <g10Style>0</g10Style>
  <minToolSlot>0</minToolSlot>
  <maxToolSlot>255</maxToolSlot>
  <g4WholeWord>P</g4WholeWord>
  <g4FracWord>P</g4FracWord>
  <startupGCode>G21</startupGCode>
  <postComment>Mach3 Mill, Metric, GWE Default Post</postComment>
  <latheXAxisDiameter>false</latheXAxisDiameter>
  <M98Style>M98P1L1</M98Style>
  <M98TryNForO>false</M98TryNForO>
  <M98ReportUndef>true</M98ReportUndef>
  <M98MaxNest>16</M98MaxNest>
  <M99IgnorePNum>true</M99IgnorePNum>
  <fullLineComment>%</fullLineComment>
  <numpost>
    <item>
      <letter>O</letter>
      <pattern>&gt;5</pattern>
      <hint>O-word identifies subprogram number</hint>
    </item>
    <item>
      <letter>N</letter>
      <pattern>&gt;5</pattern>
      <hint>N-word identifies block number.</hint>
    </item>
    <item>
      <letter>G</letter>
      <pattern>&gt;2[.]&gt;1</pattern>
      <hint>G-word specifies digits to identify a g-code.</hint>
    </item>
    <item>
      <letter>X</letter>
      <pattern>-&gt;9[.]&gt;9</pattern>
      <hint>X is used with XYZIJK words for coordinates.</hint>
    </item>
    <item>
      <letter>A</letter>
      <pattern>-&gt;9[.]&gt;9</pattern>
      <hint>A is used with ABC words for rotary axis coordinates.</hint>
    </item>
    <item>
      <letter>F</letter>
      <pattern>-&gt;9[.]&gt;9</pattern>
      <hint>F-word is for feedrates.</hint>
    </item>
    <item>
      <letter>S</letter>
      <pattern>&gt;6[.]&gt;6</pattern>
      <hint>S-word specifies spindle speed.</hint>
    </item>
    <item>
      <letter>M</letter>
      <pattern>&gt;3 0</pattern>
      <hint>M-word specifies digits to identify an m-code.</hint>
    </item>
    <item>
      <letter>T</letter>
      <pattern>&gt;3 0</pattern>
      <hint>T-word specifies tools.</hint>
    </item>
    <item>
      <letter>#</letter>
      <pattern>&gt;4</pattern>
      <hint># and its digits specify a macro variable.</hint>
    </item>
  </numpost>
</post>